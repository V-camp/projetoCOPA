generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model times {
  id        Int    @default(autoincrement()) @id
  nomeDoPais     String @unique
  qtdDeJogadores Int
  qtdDeJogadores  String
  treinador: String
  capitao: String
  qtdDeCartaoVermelho: Int
  qtdDeCartaoAmarelho: Int
  estaEmJogo: String
  grupoPertencente: String
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
  authorId  Int
  author    User    @relation(fields: [authorId], references: [id])
}
