generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model times {
    id                  Int    @default(autoincrement()) @id
    nomedopais          String @unique
    qtddejogadores      Int
    treinador           String
    capitao             String
    qtddecartaovermelho Int
    qtddecartaoamarelho Int
    estaemjogo          String
    grupopertencente    String
}

model teste {
    id                  Int    @default(autoincrement()) @id
    String              String 
}
