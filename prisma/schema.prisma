generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model times {
  id        Int    @default(autoincrement()) @id
  nomeDoPais     String @unique
  qtdDeJogadores Int
  treinador String
  capitao String
  qtdDeCartaoVermelho Int
  qtdDeCartaoAmarelho Int
  estaEmJogo String
  grupoPertencente String
}
