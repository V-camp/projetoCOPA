{
  "name": "projeto-copa",
  "private": true,
  "version": "0.0.1",
  "description": "Projeto Copa",
  "author": "Wedsley Fernando da Silva Milouchine <wedsleyfernando@gmail.com>,Vinicius Oliveira Campos de Souza <vinicius.campos2002@gmail.com>, Eduardo Oliveira Batista, Fernando Aramis Colque  ",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=localhost:8083 ./bin/www",
    "dev-docker": "../node_modules/.bin/nodemon --inspect=0.0.0.0:9229 ./bin/www",
    "build": "tsc -p .",
    "start-watch": "nodemon index.js --inspect=0.0.0.0:9229",
    "start-wait-debuger": "nodemon index.js --inspect-brk=0.0.0.0:9229",
    "test": "cross-env NODE_ENV=test PORT=8081 mocha --timeout 10000 --exit --inspect=0.0.0.0:9230",
    "test-jest": "jest --testPathPattern=ut.test\\.ts$ --logHeapUsage --runInBand --detectOpenHandles --forceExit",
    "test-watch": "nodemon --exec \"npm test\"",
    "test-wait-debuger": "cross-env NODE_ENV=test PORT=8081 mocha --no-timeouts --exit --inspect-brk=0.0.0.0:9230",
    "start-windows": "tsc ; node --inspect=localhost:8083 ./bin/www"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage/typescript",
    "coverageReporters": [
      "json",
      "html",
      "lcov"
    ],
    "setupFiles": [
      "<rootDir>/spec/test/.envTest.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/_tests_/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ],
    "testResultsProcessor": "jest-somar-reporter"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.4",
    "body-parser": "^1.20.1",
    "express": "^4.17.2",
    "g": "^2.0.1",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "test": "^3.2.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "jest-mock-extended": "^3.0.1",
    "mocha": "^9.1.4",
    "nodemon": "^2.0.15",
    "prisma": "^4.5.0",
    "typescript": "^4.8.4"
  }
}
